{
  "hosting": {
    "target": "landing-app",
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "rewrites": [
      {
        "source": "/link/**",
        "dynamicLinks": true
      }
    ],
    "redirects": [
      {
        "regex": "/weblink*",
        "destination": "https://schulplaner.pro",
        "type": 302
      },
      {
        "regex": "/download*",
        "destination": "https://schulplaner.pro/#/download",
        "type": 302
      },
      {
        "regex": "/privacy*",
        "destination": "https://schulplaner.pro/#/privacy",
        "type": 302
      },
      {
        "regex": "/datenschutz*",
        "destination": "https://schulplaner.pro/#/privacy",
        "type": 302
      },
      {
        "regex": "/support*",
        "destination": "https://schulplaner.pro/#/donate",
        "type": 302
      },
      {
        "regex": "/donate*",
        "destination": "https://schulplaner.pro/#/donate",
        "type": 302
      },
      {
        "regex": "/about*",
        "destination": "https://schulplaner.pro/#/about",
        "type": 302
      },
      {
        "regex": "/impressum*",
        "destination": "https://schulplaner.pro/#/impressum",
        "type": 302
      },
      {
        "regex": "/opensource*",
        "destination": "https://schulplaner.pro/#/opensource",
        "type": 302
      },
      {
        "regex": "/web*",
        "destination": "https://web.schulplaner.pro",
        "type": 302
      },
      {
        "regex": "/discord*",
        "destination": "https://discord.gg/m6fhx2RSTh",
        "type": 302
      },
      {
        "regex": "/assets/logo",
        "destination": "https://firebasestorage.googleapis.com/v0/b/mainprojects-32581.appspot.com/o/appdata%2Fappicon_schulplaner_png.png?alt=media&token=e0eacab3-2884-48f7-be33-7fced4d38366",
        "type": 301
      }
    ]
  }
}